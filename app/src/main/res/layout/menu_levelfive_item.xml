<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_200sdp"
        android:background="@drawable/bc"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_180sdp"
            android:layout_margin="@dimen/_10sdp"
            android:background="@drawable/boxitems">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageView5"
                android:layout_width="@dimen/_70sdp"
                android:layout_height="@dimen/_70sdp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginStart="11dp"
                android:layout_marginTop="12dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="@dimen/_150sdp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_140sdp"
                android:layout_marginTop="@dimen/_8sdp"
                android:text="TextView"
                android:textColor="#504f4f"
                android:textStyle="bold|italic" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="@dimen/_200sdp"
                android:layout_height="@dimen/_40sdp"
                android:text="TextView"
                android:textColor="#838181"
                android:textSize="@dimen/_10sdp"
                android:textStyle="bold|italic"
                android:layout_below="@+id/textView7"
                android:layout_toRightOf="@+id/imageView5"
                android:layout_toEndOf="@+id/imageView5"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp" />

        </RelativeLayout>

        <Button
            android:id="@+id/textView4"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_alignBottom="@+id/relativeLayout2"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="#c62d2d"
            android:layout_marginLeft="@dimen/_15sdp"
            android:text="delete"
            android:onClick="delete"
            android:textColor="#fff"
            android:textSize="@dimen/_10sdp"
            android:textStyle="bold" />
        <Button
            android:id="@+id/textView5"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_toRightOf="@+id/textView4"
            android:layout_alignBottom="@+id/relativeLayout2"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="#c62d2d"
            android:layout_marginLeft="@dimen/_5sdp"
            android:text="edit"
            android:textColor="#fff"
            android:textStyle="bold" />
        <RelativeLayout
            android:id="@+id/textView6"
            android:layout_width="@dimen/_170sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_toRightOf="@+id/textView5"
            android:layout_alignBottom="@+id/relativeLayout2"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="@drawable/bar"
            android:layout_marginLeft="@dimen/_10sdp"
            android:text="edit"
            android:textColor="#fff"
            android:textStyle="bold" >
            <TextView
                android:id="@+id/x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="total"
                android:textStyle="bold"
                android:textColor="#c71919"
                android:textSize="@dimen/_20sdp"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/_20sdp"/>

            <TextView
                android:id="@+id/tot"
                android:layout_width="@dimen/_140sdp"
                android:layout_height="wrap_content"
                android:text="$ "
                android:layout_toRightOf="@+id/x"
                android:textStyle="bold"
                android:textColor="#c71919"
                android:textSize="@dimen/_20sdp"
                android:layout_centerVertical="true"
                android:paddingLeft="@dimen/_35sdp"/>


        </RelativeLayout>

        <TextView
            android:id="@+id/idtxt2"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="cjkdbckjbdkdvbd"
            android:visibility="gone" />


    </RelativeLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginTop="95dp"
        android:text="price"
        android:textColor="#000"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="23dp"
        android:textColor="#f10c0c"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView10"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_toLeftOf="@+id/textView9"
        android:layout_toStartOf="@+id/textView9"
        android:text="quantity"
        android:textColor="#000"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:text="x"
        android:textColor="#ed1010"
        android:textStyle="bold"/>



    <TextView
        android:id="@+id/ids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_toLeftOf="@+id/textView11"
        android:layout_toStartOf="@+id/textView11"
        android:text="TextView"
        android:visibility="gone"/>
</RelativeLayout>